{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 4,
  "vueIndentScriptAndStyle": true,
 "useTabs": false,
 "trailingComma": "es5",
 "bracketSpacing": true,
 "arrowParens": "always",
 "endOfLine": "lf",
 "plugins": ["prettier-plugin-tailwindcss"]
}
